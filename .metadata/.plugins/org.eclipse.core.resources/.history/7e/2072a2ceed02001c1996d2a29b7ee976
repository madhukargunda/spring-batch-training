package study.pattern.batchjobrepository;

import org.springframework.batch.core.Job;
import org.springframework.batch.core.launch.JobLauncher;
import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

public class SpringBatchDatabaseJobRepository {

	public static void main(String[] args) {
		
		ApplicationContext context = new ClassPathXmlApplicationContext("student-data-processing-dbrepository-context.xml");
		JobLauncher jobLauncher = (JobLauncher) context.getBean("jobLauncher");
		Job job = (Job) context.getBean("");
	
	}
	
}
